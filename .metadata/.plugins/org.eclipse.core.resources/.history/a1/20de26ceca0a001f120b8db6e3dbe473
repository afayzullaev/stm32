#include "app_main.hpp"
#include "gpio.hpp"


void app_main(void)
{
	Gpio greenLed(12, GPIOD, OUTPUT);
	Gpio redLed(13, GPIOD, OUTPUT);
	Gpio blueLed(14, GPIOD, OUTPUT);
	Gpioy yellowLed(15, GPIOD, OUTPUT);
	while(true)
	{
		greenLed.Write(HIGH);
		HAL_Delay(1000);
		greenLed.Write(LOW);
		HAL_Delay(1000);
	}

}
